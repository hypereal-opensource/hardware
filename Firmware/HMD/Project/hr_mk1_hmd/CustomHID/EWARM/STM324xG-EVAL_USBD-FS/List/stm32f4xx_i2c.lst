    514            /* Set I2Cx Own address2 */
    515            tmpreg |= (uint16_t)((uint16_t)Address & (uint16_t)0x00FE);
    516          
    517            /* Store the new register value */
    518            I2Cx->OAR2 = tmpreg;
    519          }
